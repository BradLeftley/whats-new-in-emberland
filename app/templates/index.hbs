<section class="overview-section">
  <button
    class="button"
    type="button"
    {{action "getContributors" bubbles=false}}
  >
    Get Contributors List
  </button>

  {{#if this.contributorsList}}
    <p class="contributors">
      {{#copy-me buttonClass="fetch-contributors"}}
        {{this.contributorsList}}
      {{/copy-me}}
    </p>
  {{/if}}
</section>

<section class="overview-section">
  <h2>RFCs</h2>

  <h3>New This Week</h3>
  {{pull-requests
    pullRequests=model.newRfcs
  }}

  <h3>Accepted This Week</h3>
  {{pull-requests
    pullRequests=model.mergedRfcs
  }}
</section>

<section class="overview-section">
  <h2>Pull Requests</h2>

  <h3>Landed This Week</h3>
  {{pull-requests
    pullRequests=model.mergedPulls
  }}

  <h3>Opened This Week</h3>
  {{pull-requests
    pullRequests=model.newPulls
  }}
</section>
